    <import>
        <categories>
            <category name="settings.privacy.usermap">
                <showorder>3</showorder>
                <parent>settings.privacy</parent>
            </category>
        </categories>

        <options>
            <option name="usermapField">
                <categoryname>profile.personal</categoryname>
                <optiontype>text</optiontype>
                <searchable>1</searchable>
                <visible>15</visible>
                <editable>3</editable>
            </option>

            <option name="usermapAllowEntry">
                <categoryname>settings.privacy.usermap</categoryname>
                <optiontype>boolean</optiontype>
                <editable>3</editable>
                <defaultvalue>1</defaultvalue>
            </option>
        </options>
    </import>
